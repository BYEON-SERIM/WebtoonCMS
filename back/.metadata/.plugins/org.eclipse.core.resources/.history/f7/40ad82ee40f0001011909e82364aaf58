package com.example.cms.domain;

import java.time.LocalDateTime;

import jakarta.persistence.*;
import lombok.*;

@Entity
@Getter
@NoArgsConstructor
@AllArgsConstructor
@Builder
public class CommonCode {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    private String groupCode;  // GENRE
    private String codeValue;  // FANTASY
    private String codeName;   // 판타지
    private String attr1;      // blue-darken-2 (색상값)
    
    @Builder.Default
    private String useYn = "Y";
}